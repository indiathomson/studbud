.app-container {
  background-color: #ffc8c8;
}

.page-container {
  margin: 4em;
}
.page-container#home {
  text-align: center;
}
.page-container:not(#home) {
  display: none;
}

nav {
  width: 100%;
  background-color: #123;
  display: flex;
  justify-content: space-around;
  align-items: center;
}
nav .nav-links {
  list-style: none;
  display: flex;
}
nav .nav-links .nav.itema {
  display: inline-block;
  text-decoration: none;
  color: white;
  padding: 1em 2em;
}
nav .nav-links .nav.itema:hover {
  color: #abc;
}
nav .nav-links .nav.itema:active {
  font-weight: bold;
}
